# Airtable Configuration
# Required: Your Airtable Personal Access Token or API Key
AIRTABLE_PERSONAL_ACCESS_TOKEN=your_access_token_here
# Alternative to Personal Access Token
# AIRTABLE_API_KEY=your_api_key_here

# Required: Your Airtable Base ID
AIRTABLE_BASE_ID=your_base_id_here

# Optional: Table name overrides (only set these if your table names are different from the defaults)
# AIRTABLE_USERS_TABLE=User Profiles
# AIRTABLE_OPPORTUNITIES_TABLE=Opportunities
# AIRTABLE_RESOURCES_TABLE=Resources
# AIRTABLE_MATCHES_TABLE=Matches
# AIRTABLE_MATCHES_RESOURCES_TABLE=Matches Resources
# AIRTABLE_CONVERSATIONS_TABLE=Conversations
# AIRTABLE_MESSAGES_TABLE=Messages
# AIRTABLE_CHAT_ANALYTICS_TABLE=Chat Analytics
# AIRTABLE_DOCUMENTS_TABLE=Documents
# AIRTABLE_VETERAN_NEWS_TABLE=Veteran News
# AIRTABLE_CHAT_TRANSCRIPTS_TABLE=ChatTranscripts
# AIRTABLE_VETERAN_CHAT_TABLE=VeteranChat

# Security Settings
JWT_SECRET=your_secure_jwt_secret_here
ENCRYPTION_KEY=your_secure_encryption_key_here

# OpenAI API Configuration (for AI features)
OPENAI_API_KEY=your_openai_api_key_here

# Other Configuration
NODE_ENV=production
